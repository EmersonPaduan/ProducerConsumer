<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="telas.TelaController">
    <AnchorPane minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0">
      <children>
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
               <Label alignment="CENTER" text="Produtor" textAlignment="CENTER" />
               <TextField fx:id="txtProducer" alignment="CENTER" disable="true" focusTraversable="false" prefHeight="30.0" prefWidth="300.0" />
               <Label text="Consumidor" />
               <TextField fx:id="txtConsumer" alignment="CENTER" disable="true" focusTraversable="false" prefHeight="30.0" prefWidth="300.0" />
               <Label text="Container" />
               <TextArea fx:id="txtContainer" disable="true" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" />
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
